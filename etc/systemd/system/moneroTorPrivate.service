[Unit]
Description=Monero Tor Private
After=network.target

[Service]
User=nodo
Group=nodo
WorkingDirectory=~
RuntimeDirectory=monero
SuccessExitStatus=143
Type=simple
PIDFile=/home/nodo/monero/build/release/bin/monerod.pid
ExecStart=/bin/bash /home/nodo/execScripts/moneroTorPrivate.sh
RestartSec=30
Restart=on-failure

[Install]
WantedBy=multi-user.target